TAAPI_API_KEY=your_taapi_key_here  # From https://taapi.io (optional now, using local indicators)
HYPERLIQUID_PRIVATE_KEY=0x_your_private_key_here  # Wallet private key

# Backend URL (required for production deployments)
BACKEND_URL=http://localhost:8000  # Local dev: http://localhost:8000 | Production: https://your-backend-url.com

# LLM Provider (choose one)
LLM_PROVIDER="openai"  # openai or openrouter
OPENAI_API_KEY=your_openai_key_here  # From https://platform.openai.com
OPENROUTER_API_KEY=your_openrouter_key_here  # From https://openrouter.ai (if using openrouter)

ASSETS="BTC ETH SOL BNB ZEC EIGEN"
INTERVAL="5m"
LLM_MODEL="gpt-4o-mini"  # OpenAI: gpt-4o-mini, gpt-4o | OpenRouter: openai/gpt-4o-mini, x-ai/grok-4
RISK_PROFILE="conservative"  # conservative (default), moderate, or high (aggressive)

# Optional OpenRouter settings (only if LLM_PROVIDER=openrouter)
# OPENROUTER_REFERER=https://your-site.com
# OPENROUTER_APP_TITLE=trading-agent

HYPERLIQUID_NETWORK=testnet